<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>Recently Released Episodes (Include watched)</name>
    <match>all</match>
    <rule field="airdate" operator="inthelast">
        <value>100000</value>
    </rule>
    <order direction="descending">year</order>
</smartplaylist>